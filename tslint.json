{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "no-console": false,
      "quotemark": [true, "single"],
      "curly": [true, "ignore-same-line"],
      "space-before-function-paren": [true, "always"],
      "strictNullChecks": false,
      "object-literal-sort-keys": [
        true,
        "match-declaration-order",
        "shorthand-first"
      ]
  },
  "rulesDirectory": []
}